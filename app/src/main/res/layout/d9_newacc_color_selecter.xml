<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:gravity="center">

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:gravity="center"
		android:orientation="horizontal"
		android:layout_marginTop="5dp"
		android:layout_marginBottom="5dp">

		<com.paycheckeasy.www.paycheck.PublicClass.CircleImageView
			android:id="@+id/ac_managment_card_color_white"
			android:layout_height="35dp"
			android:layout_width="35dp"
			android:elevation="4dp"
			android:src="@drawable/radius_gradient_white"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"/>

		<com.paycheckeasy.www.paycheck.PublicClass.CircleImageView
			android:id="@+id/ac_managment_card_color_black"
			android:layout_height="35dp"
			android:layout_width="35dp"
			android:elevation="4dp"
			android:src="@drawable/radius_gradient_black"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"/>

		<com.paycheckeasy.www.paycheck.PublicClass.CircleImageView
			android:id="@+id/ac_managment_card_color_red"
			android:layout_height="35dp"
			android:layout_width="35dp"
			android:elevation="4dp"
			android:src="@drawable/radius_gradient_red"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"/>

		<com.paycheckeasy.www.paycheck.PublicClass.CircleImageView
			android:id="@+id/ac_managment_card_color_green"
			android:layout_height="35dp"
			android:layout_width="35dp"
			android:elevation="4dp"
			android:src="@drawable/radius_gradient_green"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"/>

		<com.paycheckeasy.www.paycheck.PublicClass.CircleImageView
			android:id="@+id/ac_managment_card_color_gold"
			android:layout_height="35dp"
			android:layout_width="35dp"
			android:elevation="4dp"
			android:src="@drawable/radius_gradient_gold"
			android:layout_marginLeft="10dp"
			android:layout_marginRight="10dp"/>

	</LinearLayout>

</LinearLayout>

